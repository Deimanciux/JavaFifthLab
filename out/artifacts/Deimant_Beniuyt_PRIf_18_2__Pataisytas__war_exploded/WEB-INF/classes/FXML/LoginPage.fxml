<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FXcontrollers.LoginPageController">
    <children>
        <AnchorPane layoutX="200.0" layoutY="18.0" prefHeight="364.0" prefWidth="200.0">
            <children>
                <Label layoutX="26.0" layoutY="96.0" text="Password" />
                <TextField fx:id="signInLogInName" layoutX="26.0" layoutY="53.0" />
                <Label layoutX="26.0" layoutY="36.0" text="Username" />
                <TextField fx:id="signInPassword" layoutX="26.0" layoutY="119.0" />
                <Button fx:id="signIn" layoutX="27.0" layoutY="170.0" mnemonicParsing="false" onAction="#validateUser" prefHeight="25.0" prefWidth="149.0" text="Sign In" />
                <Button fx:id="register" layoutX="27.0" layoutY="325.0" mnemonicParsing="false" onAction="#sendToRegistration" prefHeight="25.0" prefWidth="149.0" text="Don't have account? " />
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
