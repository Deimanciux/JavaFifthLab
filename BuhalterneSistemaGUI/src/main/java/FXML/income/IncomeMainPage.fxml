<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FXcontrollers.income.IncomeMainPageController">
    <children>
        <AnchorPane prefHeight="447.0" prefWidth="613.0">
            <children>
                <MenuBar layoutY="2.0" prefHeight="25.0" prefWidth="613.0">
                    <menus>
                        <Menu mnemonicParsing="false" text="Income Managing">
                            <items>
                                <MenuItem fx:id="create" mnemonicParsing="false" onAction="#createIncome" text="Create" />
                                <MenuItem fx:id="update" mnemonicParsing="false" onAction="#updateIncome" text="Update" />
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Previous Page">
                            <items>
                                <MenuItem fx:id="goToCategories" mnemonicParsing="false" onAction="#returnToCategories" text="Categories " />
                            </items>
                        </Menu>
                    </menus>
                </MenuBar>
                <AnchorPane layoutY="27.0" prefHeight="200.0" prefWidth="200.0">
                    <children>
                        <ListView fx:id="incomeList" prefHeight="427.0" prefWidth="241.0" />
                    </children>
                </AnchorPane>
                <AnchorPane layoutX="240.0" layoutY="27.0" prefHeight="111.0" prefWidth="363.0">
                    <children>
                        <Button fx:id="viewDetails" layoutX="60.0" layoutY="17.0" mnemonicParsing="false" onAction="#viewIncomeDetails" prefHeight="25.0" prefWidth="213.0" text="View  Income Details" />
                        <Button fx:id="delete" layoutX="60.0" layoutY="56.0" mnemonicParsing="false" onAction="#deleteIncome" prefHeight="25.0" prefWidth="213.0" text="Delete" />
                    </children>
                </AnchorPane>
                <AnchorPane layoutX="240.0" layoutY="131.0" prefHeight="269.0" prefWidth="363.0">
                    <children>
                        <ListView fx:id="showData" layoutY="7.0" prefHeight="317.0" prefWidth="372.0" />
                    </children>
                </AnchorPane>
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
