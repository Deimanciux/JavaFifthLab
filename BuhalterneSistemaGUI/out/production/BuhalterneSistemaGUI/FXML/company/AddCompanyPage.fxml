<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FXcontrollers.company.AddCompanyPageController">
    <children>
        <AnchorPane prefHeight="400.0" prefWidth="606.0">
            <children>
                <Label layoutX="40.0" layoutY="21.0" text="Add new Company ">
